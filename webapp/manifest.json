{
	"_version": "1.2.0",
	"sap.app": {
		"_version": "1.2.0",
		"id": "com.g4s",
		"type": "application",
		"i18n": "i18n/messageBundle.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "https://cors-anywhere.herokuapp.com/http://office.netlife.hu:8181/futarfioriodataprovider/courierdata.svc/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		},
		"sourceTemplate": {
			"id": "sap.ui.ui5-template-plugin.1worklist",
			"version": "1.36.2"
		},
		"crossNavigation": {
			"inbounds": {}
		}
	},
	"sap.ui": {
		"_version": "1.2.0",
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://task",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_bluecrystal"]
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"rootView": {
			"viewName": "com.g4s.view.App",
			"type": "XML",
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.36.0",
			"libs": {
				"sap.ui.core": {
					"minVersion": "1.36.0"
				},
				"sap.m": {
					"minVersion": "1.36.0"
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.g4s.i18n.i18n"
				}
			},
			"": {
				"dataSource": "mainService",
				"settings": {
					"metadataUrlParams": {
						"sap-documentation": "heading"
					}
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.g4s.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"bypassed": {
					"target": ["notFound"]
				},
				"async": true
			},
			"routes": [{
				"pattern": "",
				"name": "worklist",
				"target": ["worklist"]
			}, {
				"name": "bevetMaster",
				"pattern": "bevetMaster",
				"greedy": false,
				"target": ["bevetMaster"]
			}, {
				"name": "leadasMaster",
				"pattern": "leadasMaster",
				"greedy": false,
				"target": ["leadasMaster"]
			}, {
				"name": "leadasDetail",
				"pattern": "leadasDetail/{addressPath}",
				"greedy": false,
				"target": ["leadasDetail"]
			}],
			"targets": {
				"worklist": {
					"viewName": "Worklist",
					"viewId": "worklist"
				},
				"object": {
					"viewName": "Object",
					"viewId": "object"
				},
				"objectNotFound": {
					"viewName": "ObjectNotFound",
					"viewId": "objectNotFound"
				},
				"notFound": {
					"viewName": "NotFound",
					"viewId": "notFound"
				},
				"bevetMaster": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "bevetMaster",
					"viewId": "bevetMaster"
				},
				"leadasMaster": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "leadasMaster",
					"viewId": "leadasMaster"
				},
				"leadasDetail": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "leadasDetail",
					"viewId": "leadasDetail"
				}
			}
		}
	}
}